{
  "name": "@gftdcojp/ai-gftd-ontology-typebox",
  "version": "1.0.0",
  "description": "DoDAF 2.0 Ontology validation with TypeBox and JSON-LD",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "dodaf-cli": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsup && pnpm run generate:context && pnpm run generate:shacl && pnpm run generate:owl",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "CI=1 vitest --run --reporter=dot --ui=false --no-watch --passWithNoTests",
    "type-check": "tsc --noEmit",
    "generate:context": "node scripts/generate-context.js",
    "generate:shacl": "node scripts/generate-shacl.js",
    "generate:owl": "node scripts/generate-owl.js",
    "generate:semantic": "pnpm run generate:context && pnpm run generate:shacl && pnpm run generate:owl",
    "serve:ontology": "tsx src/server/start.ts"
  },
  "keywords": [
    "dodaf",
    "ontology",
    "typebox",
    "jsonld",
    "typescript"
  ],
  "author": "GFTD Corp",
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.17.0",
  "dependencies": {
    "@gftdcojp/resourcebox": "0.2.1",
    "@orpc/client": "^1.10.0",
    "@orpc/server": "^1.10.0",
    "@rdfjs/data-model": "^2.0.1",
    "@rdfjs/types": "^1.1.0",
    "commander": "^14.0.1",
    "hono": "^4.10.2",
    "jsonld": "^8.3.3",
    "n3": "^1.17.3",
    "rdf-validate-shacl": "^0.4.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/jsonld": "^1.5.15",
    "@types/n3": "^1.26.0",
    "@types/node": "^24.9.0",
    "@types/rdfjs__data-model": "^2.0.9",
    "fast-xml-parser": "^5.3.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist"
  ]
}